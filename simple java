
public class T1chromelaunching {

	
	public static void main(String[] args)
	{
		System.setProperty("webdriver.chrome.driver", "E:\\Testing\\chromedriver_win32\\chromedriver.exe");
		WebDriver driver=new ChromeDriver();
		driver.get("http://www.google.co.in");
		system.out.println("HI);
		System.setProperty("webdriver.gecko.driver","E:\\Testing\\geckodriver-v0.16.1-win64\\geckodriver.exe");
		WebDriver driver=new FirefoxDriver();
		driver.get("http:/www.google.co.in");
		

	}

}

